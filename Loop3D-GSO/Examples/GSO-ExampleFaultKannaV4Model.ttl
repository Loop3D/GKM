kan:Simpson_fault
  a gsfa:Thrust_Fault ;
  gsoc:isParticipantIn kan:Simpson_fault_displacement ;
  gsrl:crosscuts kan:N15_fault ;
  gsrl:crosscuts kan:Nublet_fault ;
  rdfs:label "Simpson Thrust fault"@en ;
.
kan:Simpson_fault_displacement
  a gsog:Geologic_Event ;
  gsoc:hasConstituent [
