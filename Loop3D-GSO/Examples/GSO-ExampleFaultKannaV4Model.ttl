      rdfs:label "Simpson fault faulting process"@en ;
    ] ;
  gsoc:timeYoungerThan kan:N15_fault_displacement ;
  gsoc:timeYoungerThan kan:Nublet_fault_displacement ;
  rdfs:comment ""@en ;
  rdfs:label "Displacment on Simpson Fault"@en ;
.
kan:T07_fault
