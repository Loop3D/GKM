                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty gsoc:hasQuality ;
                                 owl:allValuesFrom gsoc:Temporal_Quality
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty gsoc:hasQuality ;
                                 owl:allValuesFrom [ rdf:type owl:Restriction ;
                                                     owl:onProperty gsoc:hasValue ;
