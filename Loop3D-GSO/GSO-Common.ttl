                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty gsoc:inheresIn ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass gsoc:Particular
                              ] ;
              owl:disjointWith gsoc:Notional_Endurant ,
