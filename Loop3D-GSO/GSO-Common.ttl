                                    owl:onProperty gsoc:hasQuality ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass [ owl:intersectionOf ( gsoc:State_Of_Matter
                                                                       [ rdf:type owl:Restriction ;
