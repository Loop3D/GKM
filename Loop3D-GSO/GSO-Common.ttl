                          rdf:type rdf:List ;
                          rdf:first :State_Of_Matter ;
                          rdf:rest [
                              rdf:type rdf:List ;
                              rdf:first [
                                  rdf:type owl:Restriction ;
                                  owl:allValuesFrom :Fluid ;
                                  owl:onProperty :hasValue ;
                                ] ;
                              rdf:rest () ;
                            ] ;
                        ] ;
                    ] ;
                  owl:onProperty :hasQuality ;
                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                ] ;
              rdf:rest () ;
            ] ;
        ] ;
    ] ;
