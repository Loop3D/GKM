      owl:onProperty :hasEssentialPart ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty :hasEssentialPart ;
      owl:someValuesFrom :Endurant ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty :hostedBy ;
      owl:someValuesFrom :Particular ;
    ] ;
  owl:disjointWith :Perdurant_Feature ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf [
          rdf:type rdf:List ;
          rdf:first :Feature ;
          rdf:rest [
              rdf:type rdf:List ;
