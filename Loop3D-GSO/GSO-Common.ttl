      owl:onProperty :hasEssentialPart ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty :hasEssentialPart ;
      owl:someValuesFrom :Situation ;
    ] ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf [
          rdf:type rdf:List ;
          rdf:first :Feature ;
          rdf:rest [
              rdf:type rdf:List ;
              rdf:first [
                  rdf:type owl:Restriction ;
                  owl:onProperty :hasEssentialPart ;
                  owl:someValuesFrom :Situation ;
                ] ;
              rdf:rest () ;
            ] ;
        ] ;
    ] ;
