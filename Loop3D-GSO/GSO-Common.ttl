  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom :Notional_Endurant ;
      owl:onProperty :hasPart ;
    ] ;
  owl:disjointWith :Relator ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf [
          rdf:type rdf:List ;
          rdf:first :Nonphysical_Endurant ;
          rdf:rest [
              rdf:type rdf:List ;
              rdf:first [
                  rdf:type owl:Class ;
