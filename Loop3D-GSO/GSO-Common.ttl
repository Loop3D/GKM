                      owl:onProperty :occupiesSpaceDirectly ;
                      owl:someValuesFrom :Spatial_Region ;
                    ] ;
                ] ;
              rdf:rest () ;
            ] ;
        ] ;
    ] ;
.
:Nonphysical_Feature
