.
:Occurrence_Feature
  rdf:type owl:Class ;
  rdfs:comment "" ;
  rdfs:label "Occurrence Feature "@en ;
  rdfs:subClassOf :Perdurant_Feature ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom [
          rdf:type owl:Class ;
          owl:unionOf [
              rdf:type rdf:List ;
              rdf:first :Event ;
              rdf:rest [
                  rdf:type rdf:List ;
                  rdf:first :Process ;
                  rdf:rest () ;
                ] ;
            ] ;
