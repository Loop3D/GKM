                              rdf:type rdf:List ;
                              rdf:first [
                                  rdf:type owl:Restriction ;
                                  owl:allValuesFrom :Solid ;
                                  owl:onProperty :hasValue ;
                                ] ;
                              rdf:rest () ;
                            ] ;
                        ] ;
                    ] ;
                  owl:onProperty :hasQuality ;
                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                ] ;
              rdf:rest () ;
            ] ;
        ] ;
    ] ;
.
:Spatial_Location
