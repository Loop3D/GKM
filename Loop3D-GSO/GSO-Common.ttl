      owl:onProperty :hasEssentialPart ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty :hasEssentialPart ;
      owl:someValuesFrom :Inherant ;
    ] ;
  owl:equivalentClass [
