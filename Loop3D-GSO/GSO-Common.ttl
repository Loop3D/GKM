      owl:intersectionOf [
          rdf:type rdf:List ;
          rdf:first :Quality ;
          rdf:rest [
              rdf:type rdf:List ;
              rdf:first [
                  rdf:type owl:Restriction ;
                  owl:onClass :Perdurant ;
                  owl:onProperty :isQualityOf ;
                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                ] ;
              rdf:rest () ;
            ] ;
        ] ;
    ] ;
