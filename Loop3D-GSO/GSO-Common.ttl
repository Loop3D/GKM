###  https://w3id.org/gso/common/Inherant
gsoc:Inherant rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( gsoc:Nonphysical_Endurant
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty gsoc:inheresIn ;
                                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass gsoc:Particular
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf gsoc:Nonphysical_Endurant ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty gsoc:hasPart ;
