  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty :hasStaticPart ;
      owl:someValuesFrom :Example ;
    ] ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf [
          rdf:type rdf:List ;
          rdf:first :Relator ;
