  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsog:Epoch ;
      owl:onProperty gsoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass gsog:Era ;
      owl:onProperty gsoc:isPartOf ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
.
gsog:Porespace
  a owl:Class ;
  rdfs:subClassOf gsog:Rock_Body_Void ;
.
gsog:Pyroclastic_Granular_Material
  a owl:Class ;
  rdfs:comment "Use for description of pyroclastic rocks"@en ;
  rdfs:subClassOf gsog:Granular_Material ;
.
gsog:Rock_Body
  a owl:Class ;
  rdfs:comment "An endurant in which some Rock Material inheres, either as an 'amount of matter' (disembodied matter), or as a geologic unit."@en ;
  rdfs:label "Rock Body"@en ;
  rdfs:subClassOf gsoc:Physical_Endurant ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Amount_Of_Matter ;
      owl:onProperty gsoc:hasConstituent ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsoc:Physical_Feature ;
      owl:onProperty gsoc:hosts ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first gsog:Geologic_Unit ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first gsog:Rock_Body_Part ;
                  rdf:rest () ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gsoc:hasPart ;
    ] ;
.
gsog:Rock_Body_Bottom
  a owl:Class ;
  rdfs:comment """Rock Body Bottom (RBB):
The bottom surface of a rock body.
RBT (x) <--> RBD (x) ^ exists z forall y [isPartOf (y,z) ^ hasHost (z,x) -> above (y,x) v isPartOf (y,x)]"""@en ;
  rdfs:label "Rock Body Bottom"@en ;
  rdfs:subClassOf gsog:Rock_Body_Boundary ;
.
gsog:Rock_Body_Boundary
  a owl:Class ;
  rdfs:comment """a physical boundary hosted by a rock body.
RBD (x) <--> SF (x) ^ exists y [RB (y) ^ hosts (y,x)] 
The exterior-facing material of a rock body."""@en ;
  rdfs:label "Rock Body Boundary"@en ;
  rdfs:subClassOf gsoc:Physical_Boundary ;
  rdfs:subClassOf gsog:Rock_Body_Part ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsog:Rock_Body_Boundary ;
      owl:onProperty gsoc:hasPart ;
    ] ;
.
gsog:Rock_Body_Part
  a owl:Class ;
  rdfs:label "Rock Body Part"@en ;
  rdfs:subClassOf gsog:Rock_Body ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsog:Rock_Body_Part ;
      owl:onProperty gsoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsog:Rock_Material ;
      owl:onProperty gsoc:hasConstituent ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isPartOf ;
      owl:someValuesFrom gsog:Rock_Body ;
    ] ;
.
gsog:Rock_Body_Top
  a owl:Class ;
  rdfs:comment """Rock Body Top (RBT):
The top surface of a rock body.
RBT (x) <--> RBD (x) ^ exists z forall y [(isPartOf (y,z) ^ hasHost (z,x)) -> (below (y,x) v isPartOf (y,x))]"""@en ;
  rdfs:label "Rock Body Top"@en ;
  rdfs:subClassOf gsog:Rock_Body_Boundary ;
.
gsog:Rock_Body_Void
  a owl:Class ;
  rdfs:label "Rock Body Void"@en ;
  rdfs:subClassOf gsoc:Physical_Void ;
.
gsog:Rock_Material
  a owl:Class ;
  rdfs:comment "hook into CGI simple lithology ontology. SMR has one, and Clinton Smyth has an extension of that with more rock types."@en ;
  rdfs:label "Rock Material"@en ;
  rdfs:subClassOf gsoc:Amount_Of_Matter ;
  rdfs:subClassOf gsog:Rock_Body ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsog:Rock_Body_Part ;
      owl:onProperty gsoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first gsoc:Element ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first gsog:Glass ;
                  rdf:rest [
                      a rdf:List ;
                      rdf:first gsog:Granular_Material ;
                      rdf:rest [
                          a rdf:List ;
                          rdf:first gsog:Mineral ;
                          rdf:rest () ;
                        ] ;
                    ] ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gsoc:hasConstituent ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first gsog:Geologic_Unit ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first gsog:Rock_Body_Part ;
                  rdf:rest () ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gsoc:isConstituentOf ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass gsog:Rock_Body ;
      owl:onProperty gsoc:isPartOf ;
      owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
    ] ;
.
gsog:Series
  a owl:Class ;
  rdfs:subClassOf gsog:Chronostratigraphic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:realizes ;
      owl:someValuesFrom gsog:Epoch ;
    ] ;
.
gsog:Specimen
  a owl:Class ;
  rdfs:label "Rock Material Specimen"@en ;
  rdfs:subClassOf gsog:Rock_Body_Part ;
.
gsog:Stage
  a owl:Class ;
  rdfs:comment "The stage includes all rocks formed during an age. A stage is normally the lowest ranking unit in the chronostratigraphic hierarchy that can be recognized on a global scale. The stage has been called the basic working unit of chronostratigraphy because it is suited in scope and rank to the practical needs and purposes of intraregional chronostratigraphic classification. It is a subdivision of a series."@en ;
  rdfs:subClassOf gsog:Chronostratigraphic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:isPartOf ;
      owl:someValuesFrom gsog:Series ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:realizes ;
      owl:someValuesFrom gsog:Age_Rank ;
    ] ;
.
gsog:Stratigraphic_Point
  a owl:Class ;
  rdfs:comment "A spatially restricted part of a Contact feature, typically located by a point location. Could be thought of as an instance of a Contact, or a sample of a Contact."@en ;
  rdfs:label "Stratigraphic Point"@en ;
  rdfs:subClassOf gsog:Contact ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasReference ;
      owl:someValuesFrom gsog:Stratotype ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsog:hasSpatialProperty ;
      owl:someValuesFrom gsoc:Spatial_Extent ;
    ] ;
.
gsog:Stratigraphic_Section
  a owl:Class ;
  rdfs:label "Stratigraphic Section"@en ;
  rdfs:subClassOf gsog:Rock_Body_Part ;
.
gsog:Stratotype
  a owl:Class ;
  rdfs:label "Stratotype"@en ;
  rdfs:subClassOf gsog:Stratigraphic_Section ;
.
gsog:Sub-Period
  a owl:Class ;
  rdfs:comment "Named rank for geochronologic time interval. Sub-Periods may be subdivided into Epochs. Sub-Periods are grouped into a period."@en ;
  rdfs:subClassOf gsog:Geologic_Time_Interval ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsog:Epoch ;
      owl:onProperty gsoc:hasPart ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onClass gsog:Period ;
      owl:onProperty gsoc:isPartOf ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
.
gsog:Substage
  a owl:Class ;
  rdfs:subClassOf gsog:Chronostratigraphic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:realizes ;
      owl:someValuesFrom gsog:Age_Rank ;
    ] ;
.
gsog:Subtractive_Process
  a owl:Class ;
  rdfs:label "Subtractive Process"@en ;
  rdfs:subClassOf gsog:Geologic_Process ;
.
gsog:Supereon
  a owl:Class ;
  rdfs:comment "hight ranking geochronlogic time interval, parts are eons"@en ;
  rdfs:label "Supereon"@en ;
  rdfs:subClassOf gsog:Geologic_Time_Interval ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom gsog:Eon ;
      owl:onProperty gsoc:hasPart ;
    ] ;
.
gsog:Supergroup
  a owl:Class ;
  rdfs:subClassOf gsog:Lithostratigraphic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf [
              a rdf:List ;
              rdf:first gsog:Complex ;
              rdf:rest [
                  a rdf:List ;
                  rdf:first gsog:Formation ;
                  rdf:rest [
                      a rdf:List ;
                      rdf:first gsog:Group ;
                      rdf:rest () ;
                    ] ;
                ] ;
            ] ;
        ] ;
      owl:onProperty gsoc:hasPart ;
    ] ;
.
gsog:System
  a owl:Class ;
  rdfs:subClassOf gsog:Chronostratigraphic_Unit ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:realizes ;
      owl:someValuesFrom gsog:Period ;
    ] ;
.
gsog:Transformation
  a owl:Class ;
  rdfs:comment "process that  changes the characteristics of matter within the model space"@en ;
  rdfs:label "Transformation Process"@en ;
  rdfs:subClassOf gsog:Geologic_Process ;
.
gsog:Xenolith
  a owl:Class ;
  rdfs:label "Xenolith"@en ;
  rdfs:subClassOf gsog:Rock_Body_Part ;
.
gsog:bolide_impact
  a owl:Class ;
  dcterms:source "this vocabulary"@en ;
  rdfs:comment "The impact of an extraterrestrial body on the surface of the earth"@en ;
  rdfs:label "bolide impact"@en ;
  rdfs:subClassOf gsog:Geologic_Event ;
.
gsog:cometary_impact
  a owl:Class ;
  dcterms:source "this vocabulary"@en ;
  rdfs:comment "The impact of a comet on the surface of the earth"@en ;
  rdfs:label "cometary impact"@en ;
  rdfs:subClassOf gsog:bolide_impact ;
.
gsog:hasAgeDate
  rdfs:comment "property to assigne a Geologic_Time_Date to a Perdurant."@en ;
  rdfs:range gsoc:Time_Instant ;
  rdfs:label "has age date"@en ;
.
gsog:hasBoundaryThickness
  a owl:ObjectProperty ;
  rdfs:comment "thickness (length) of boundary zone at a gradational contact "@en ;
  rdfs:domain gsog:Gradational_Contact_Zone ;
  rdfs:label "has boundary thickness "@en ;
  rdfs:range gsop:Thickness ;
  rdfs:subPropertyOf gsog:isGeologicallyRelatedTo ;
.
gsog:hasDate
  a owl:DatatypeProperty ;
  rdfs:comment "numeric value for a time coordinate. Associated hasUOM property asserts the units of measure; the temporal reference system used to assert dates should be explained in metadata for the data instances."@en ;
  rdfs:subPropertyOf gsoc:hasNumericValue ;
.
gsog:hasDisplacement
  a owl:ObjectProperty ;
  rdfs:domain [
      a owl:Class ;
      owl:unionOf [
          a rdf:List ;
          rdf:first gsog:Ductile_Shear_Zone ;
          rdf:rest [
              a rdf:List ;
              rdf:first gsog:Fault ;
              rdf:rest () ;
            ] ;
        ] ;
    ] ;
  rdfs:range gsop:Displacement ;
  rdfs:subPropertyOf gsoc:hasPhysicalProperty ;
.
gsog:hasMaterialConstituent
  a owl:ObjectProperty ;
  rdfs:range gsog:Rock_Material ;
  rdfs:subPropertyOf gsoc:hasConstituent ;
.
gsog:hasOlderAge
  a owl:ObjectProperty ;
    rdfs:range gsoc:Time_Interval ;
  rdfs:subPropertyOf gsoc:hasTemporalProperty ;
.
gsog:hasOlderBound
  a owl:ObjectProperty ;
  rdfs:label "has Older Bounding Age"@en ;
  rdfs:range gsoc:Time_Instant ;
  rdfs:subPropertyOf gsoc:hasTemporalProperty ;
.
gsog:hasOlderHost
  a owl:ObjectProperty ;
  rdfs:subPropertyOf gsoc:hostedBy ;
.
gsog:hasOrientation
  a owl:ObjectProperty ;
  rdfs:comment "property linking a geologic class with an Orientation property"@en ;
  rdfs:domain gsog:Geologic_Structure ;
  rdfs:label "definingElement"@en ;
  rdfs:subPropertyOf gsoc:hasPhysicalProperty ;
.
gsog:hasYoungerAge
  a owl:ObjectProperty ;
    rdfs:comment "assign a younger age boundary for a time interval as a time interval, e.g. Jurassic thru Cretaceous, younger age is Cretaceous."@en ;
  rdfs:label "younger bounding time interval"@en ;
    rdfs:range gsoc:Time_Interval ;
  rdfs:subPropertyOf gsoc:hasTemporalProperty ;
.
gsog:hasYoungerBound
