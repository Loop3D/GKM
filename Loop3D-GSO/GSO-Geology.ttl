  rdfs:comment "assign a younger age boundary as a time instant for a time interval"@en ;
  rdfs:label "younger bounding time position"@en ;
  rdfs:range gsoc:Time_Instant ;
  rdfs:subPropertyOf gsoc:hasTemporalProperty ;
.
gsog:hasYoungerHost
  a owl:ObjectProperty ;
    rdfs:comment "for a feature that is hosted by multiple entities, e.g. a contact, specify the younger hosting entity."@en ;
  rdfs:label "younger host"@en ;
  rdfs:subPropertyOf gsoc:hostedBy ;
.
gsog:intrudes
  a owl:ObjectProperty ;
  rdfs:comment "subject rock body intrudes object rock body. Implies that subject rock body is igneous rock, and that object rock body is older than subject rock body"@en ;
  rdfs:label "intrudes"@en ;
  rdfs:subPropertyOf gsog:isGeologicallyRelatedTo ;
.
gsog:isGeologicallyRelatedTo
  a owl:ObjectProperty ;
  rdfs:label "geologically related to"@en ;
  rdfs:subPropertyOf gsoc:isPhysicallyRelatedTo ;
.
gsog:isPartOfTimeScale
  a owl:ObjectProperty ;
  rdfs:comment "object property linking a time interval to a time scale"@en ;
  rdfs:label "in Time Scale "@en ;
  rdfs:range gsog:Geologic_Time_Scale ;
  rdfs:subPropertyOf gsoc:isPartOf ;
.

