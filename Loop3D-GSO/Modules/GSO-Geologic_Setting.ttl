gsen:ontology
  rdf:type owl:Ontology ;
  dct:created "2021-03-26"^^xsd:date ;
  dct:creator gsoc:boyan_brodaric ;
  dct:creator gsoc:stephen_richard ;
  dct:creator "CGI Geoscience Terminology Working Group"@en ;
  dct:license <https://creativecommons.org/licenses/by/4.0/legalcode> ;
  dct:modified "2021-03-26"^^xsd:date ;
  dct:publisher "ARC Loop3D project;  https://loop3d.org/" ;
  dct:publisher "Geological Survey of Canada, Natural Resources Canada, Government of Canada" ;
    schema:codeRepository "https://github.com/Loop3D/GKM";  dct:rights  "Copyright (c) 2021 Government of Canada" ;
  rdfs:comment "Seeded by the 2016 SKOS-RDF version of the IUGS Commission for Geoscience Information (CGI) Geoscience Terminology Working Group Event Environment vocabulary to describe the physical setting within which a GeologicEvent takes place. GeologicEnvironment is construed broadly to include physical settings on the Earth surface specified by climate, tectonics, physiography or geography, and settings in the Earth’s interior specified by pressure, temperature, chemical environment, or tectonics. Environments are places. Originally implemented for use in the eventEnvironment property in GeoSciML GeologicEvent features, adapted here to align with this ontology."@en ;
  rdfs:label "Geoscience Ontology, Setting"@en ;
  dct:bibliographicCitation "Brodaric, B., and Richard, S.M., 2021, The GeoScience Ontology Reference: Geological Survey of Canada Open File 8796, 34 p., https://doi.org/10.4095/328296";
  owl:imports gsoc:ontology ;
  owl:imports gspr:ontology ;
  owl:imports gsog:ontology ;
  owl:imports gsro:ontology ;
  skos:definition "Geologic settings."@en ;
  skos:prefLabel "Geoscience Ontology, Setting"@en ;
.
