# property bindings defined here using classes from geology, common, and properties
gsog:Lineation
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hasPhysicalProperty ;
      owl:someValuesFrom gsop:Line_Orientation ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty gsoc:hostedBy ;
      owl:someValuesFrom gsog:Mineral ;
    ] ;
.


