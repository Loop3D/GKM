  skos:narrower env:distributary_mouth ;
  skos:narrower env:estuarine_delta ;
  skos:narrower env:interdistributary_bay ;
  skos:narrower env:lacustrine_delta ;
  skos:narrower env:prodelta ;
  skos:prefLabel "deltaic system setting"@en ;
