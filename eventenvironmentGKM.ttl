                                                                                                    <http://resource.geosciml.org/classifier/cgi/eventenvironment/interdistributary_bay> ,
                                                                                                    <http://resource.geosciml.org/classifier/cgi/eventenvironment/lacustrine_delta> ,
                                                                                                    <http://resource.geosciml.org/classifier/cgi/eventenvironment/prodelta> ;
